package test.math.optimization;

component ForLoopConditionsTest{
    implementation Math{
        minimize(Q^{3} x)
          Q y = x(1) * x(1) + x(2) * x(2) + x(3) * x(3);
        subject to
          for i = 1:3
            x(i) >= 0;
            0 <= 2 * x(i) <= 2;
          end
        end
    }
}