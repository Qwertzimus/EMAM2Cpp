package optimizer;

component ChainedMultiplicationAddition{
    ports in Q(-oo:oo)^{10,200} mat1,
          in Q(-oo:oo)^{10,10} mat2,
          in Q(-oo:oo)^{200,10} mat3,
          in Q(-oo:oo)^{200,10} mat4,
          out Q(-oo:oo)^{10,200} matOut;


    implementation Math{
        matOut = mat3*mat1+mat4*mat1+mat3*mat2;
    }

}