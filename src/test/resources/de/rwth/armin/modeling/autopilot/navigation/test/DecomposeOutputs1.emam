package de.rwth.armin.modeling.autopilot.navigation.test;

component DecomposeOutputs1 {
  port
    in Z (0 : 10000) path_length,
    in Q ^ {1,10000} path_gpsLat,
    in Q ^ {1,10000} path_gpsLon,

    out Q path_gpsLat0,
    out Q path_gpsLat1,
    out Q path_gpsLat2,
    out Q path_gpsLat3,
    out Q path_gpsLat4,
    out Q path_gpsLon0,
    out Q path_gpsLon1,
    out Q path_gpsLon2,
    out Q path_gpsLon3,
    out Q path_gpsLon4;

  implementation Math {
    if path_length > 0 {
      path_gpsLat0 = path_gpsLat(1,1);
      path_gpsLon0 = path_gpsLon(1,1);
    } end
    if path_length > 1 {
      path_gpsLat1 = path_gpsLat(1,2);
      path_gpsLon1 = path_gpsLon(1,2);
    } end
    if path_length > 2 {
      path_gpsLat2 = path_gpsLat(1,3);
      path_gpsLon2 = path_gpsLon(1,3);
    } end
    if path_length > 3 {
      path_gpsLat3 = path_gpsLat(1,4);
      path_gpsLon3 = path_gpsLon(1,4);
    } end
    if path_length > 4 {
      path_gpsLat4 = path_gpsLat(1,5);
      path_gpsLon4 = path_gpsLon(1,5);
    } end
  }
}
